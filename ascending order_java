import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter three numbers to print in ascending order");
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        if(a<b && a<c && b<c){
            System.out.println(a);
            System.out.println(b);
            System.out.println(c);
        }
        else if(b<a && b<c && a<c){
            System.out.println(b);
            System.out.println(a);
            System.out.println(c);

        }
        else if(c<a && c<b && a<b){
            System.out.println(c);
            System.out.println(a);
            System.out.println(b);

        }
        else{
            System.out.println("invalid");
        }
    }
}
